generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model TossHistory {
  id Int @id @default(autoincrement())
  head BigInt @default(0)
  tail BigInt @default(0)
  toss BigInt @default(0)
}
